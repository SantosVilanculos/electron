{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["./src/renderer/**/*.ts", "./src/renderer/**/*.vue", "./src/types/*.ts"],
  "compilerOptions": {
    "noEmit": true,
    "skipLibCheck": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": ["./src/renderer/*"]
    }
  }
}
